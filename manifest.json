{
  "manifest_version": 3,
  "name": "Rank-Based Seat Finder",
  "version": "1.0",
  "description": "Filter and highlight JoSAA seat data.",
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}